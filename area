print("What is the length?")
l = int(input())
print("what is the width")
w = int(input())
perimeter = (2*l) + (2*w)
area = (l*w)
print("the length of the rectangle is",l)
print("the width or the rectangle is",w)
print("the Perimeter of the rectangle is",perimeter, "u")
print("the Area of the rectangle is",area, "u^2")
